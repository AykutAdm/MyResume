@model List<Category>

<nav class="portfolio-filter isotope-filter">
    <ul>
        <li><a href="#" class="active" data-filter="*">Tüm Kategoriler</a></li>
        @foreach (var item in Model)
        {
            <li><a href="#" data-filter=".@item.CategoryId">@item.CategoryName</a></li>
        }
    </ul>
</nav>